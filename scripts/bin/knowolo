#!/usr/bin/env php
<?php

declare(strict_types=1);

require __DIR__.'/../vendor/autoload.php';

use Knowolo\Command\GeneratePlainPhpCodeCommand;
use Knowolo\Command\GenerateSerializedPhpCodeCommand;
use Knowolo\Generator\PhpCodeGenerator;
use Knowolo\Generator\SerializedPhpCodeGenerator;
use quickRdf\DataFactory;
use Symfony\Component\Console\Application;

$dataFactory = new DataFactory();
$application = new Application();

$application->add(new GeneratePlainPhpCodeCommand(new PhpCodeGenerator($dataFactory)));
$application->add(new GenerateSerializedPhpCodeCommand(new SerializedPhpCodeGenerator($dataFactory)));

$application->run();
